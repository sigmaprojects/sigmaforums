<cfoutput><h1>Role Editor</h1>#getPlugin("MessageBox").renderit()#<form method="post" action="#event.buildLink('security.roles.save')#">	<input type="hidden" name="roleID" id="roleID" value="#rc.role.getRoleID()#" />	<div>		<label for="name">Name:</label>		<input type="text" name="name" value="#rc.role.getName()#"/>	</div>	<div>		<label for="lastlogin">User Count:</label>		#rc.role.getUserCount()#	</div>			<div align="right">		<input type="button" value="Cancel" onclick="location.href='#event.buildLink('security.roles.list')#'"> 		<input type="submit" value="Save">	</div>	</form></cfoutput>