/*A general event handler. This is a dummy one, please remove and create real ones.*/component singleton=true {		property name="ORMService" inject="coldbox:plugin:ORMService";		// OPTIONAL HANDLER PROPERTIES	this.event_cache_suffix = "";	this.prehandler_only 	= "";	this.prehandler_except 	= "";	this.posthandler_only 	= "";	this.posthandler_except = "";	// Allowed HTTP Methods Ex: this.allowedMethods = {delete='POST,DELETE',index='GET'}	this.allowedMethods = {};		/**	IMPLICIT FUNCTIONS: Uncomment to use	void function preHandler(event,action) output=false{		var rc = event.getCollection();	}	void function postHandler(event,action) output=false{		var rc = event.getCollection();	}	void function onMissingAction(event,missingAction) output=false{	}	*/	void function index(event) output=false{		var rc = event.getCollection();		rc.areas = ORMService.list(entityName='Area',asQuery=false);				event.setView("admin/home");	}		void function editArea(event) output=false{		var rc = event.getCollection();		rc.area = ORMService.get("Area",event.getValue('areaid',0));		event.setView("admin/area_edit");
	}	void function saveArea(event) output=false{		var rc = event.getCollection();		if( ORMService.exists("Area",event.getValue('areaid',0)) ) {			var area = ORMService.get("Area",event.getValue('areaid'));		} else {			var area = ORMService.new("Area");		}		populateModel(area);		ORMService.save(area);		getPlugin("MessageBox").setMessage("info","Area '<strong>#Area.getTitle()#</strong>' was saved.");		setNextEvent(event='admin.index',varStruct=rc);
	}	void function editSection(event) output=false{		var rc = event.getCollection();		rc.section = ORMService.get("Section",event.getValue('sectionid',0));		rc.areas = ORMService.list(entityName='Area',asQuery=false);		event.setView("admin/section_edit");	}	void function saveSection(event) output=false{		var rc = event.getCollection();		if( ORMService.exists("Section",event.getValue('sectionid',0)) ) {			var Section = ORMService.get("Section",event.getValue('sectionid'));		} else {			var Section = ORMService.new("Section");		}		var area = ORMService.get("Area",event.getValue('areaid'));		Section.setArea(area);		populateModel(Section);		ORMService.save(Section);		getPlugin("MessageBox").setMessage("info","Section '<strong>#Section.getTitle()#</strong>' was saved.");		setNextEvent(event='admin.index',varStruct=rc);	}	/*		if( event.getValue('delete','false') is not 'false' ) {			setNextEvent(event='admin.album.remove',queryString="id=#album.getID()#",varStruct=rc);		}	*/}